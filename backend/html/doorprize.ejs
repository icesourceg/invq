<!DOCTYPE html>
<html lang="en">
    <head>
    <% include ./include/head %>
  </head>
    <body>

        <div class="container">
            <div class="row">
              <% include ./include/header %>
            </div>
            
            <div class="prize-content">
                <div class="doorprize-content-header"><%= data.title %></div>
                <div class="prize-content-body">
                  
                  <% data.rows.data.forEach(function(d){ %>
                  <div class="doorprize-column">
                    <div class="doorprize-cell">
                        <div class="doorprize-cell-details-big doorprize-cell-details">
                          <%= numpad(d.Guesthistory.id,4) %>
                        </div>
                        </div>
                    <div class="doorprize-cell">
                        <div class="doorprize-cell-details-small">
                            <strong><%= d.shop_name %></strong><br />-<br />
                            <em><%= d.name %></em><br />-<br/>

                            <%= d.city %>
                      </div>
                    </div> 
                  </div>
                  <% }) %>
                  
                </div>
            </div>
          </div>
        <% include ./include/scripts %>
        <% include ./include/footer %>
    </body>
</html>